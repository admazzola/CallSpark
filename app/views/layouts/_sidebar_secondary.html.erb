
<% if !inDemoInventorySpace %>

<li>
     <a data-toggle="collapse" data-parent="#accordion" href="#collapseDatasets" aria-expanded="true" aria-controls="collapseDatasets">
       <i class="fa fa-bars"></i> Datasets<span id="datasetsArrow" class="fa fa-chevron-circle-down pull-right"  style="display:none" ></span></a>
     <ul id="collapseDatasets" class="nav nav-second-level collapse in" aria-expanded="true">

       <li>
           <a <%= cssClassActive('calls') %> href="<%= url_for :controller => 'calls', :action => 'index' %>"><i class="fa fa-phone fa-fw" style="color:<%= color(5) %>" ></i> Support Calls </a>
       </li>
       <li>
           <a <%= cssClassActive('customer') %> href="<%= url_for :controller => 'customer', :action => 'index' %>"><i class="fa fa-users fa-fw" style="color:<%= color(4) %>"  ></i> Customers </a>
       </li>
     <li>
           <a <%= cssClassActive('company') %> href="<%= url_for :controller => 'company', :action => 'index' %>"><i class="fa fa-th-large fa-fw" style="color:<%= color(4) %>"></i> Companies </a>
       </li>


     </ul>


   </li>


  <% if( Supportlink.all.length > 0) %>

   <li>
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseTools" aria-expanded="true" aria-controls="collapseTools">
          <i class="fa fa-wrench fa-fw"></i> Support Tools<span id="toolsArrow" class="fa fa-chevron-circle-down pull-right"  style="display:none" ></span></a>
        <ul id="collapseTools" class="nav nav-second-level collapse in" aria-expanded="true">


          <% Supportlink.order(:sortorder).each do |link| %>

            <li>
                <a href="<%= link.url %>" target="_blank"><%= link.name %></a>
            </li>
             <% end %>

        </ul>
        <!-- /.nav-second-level -->
    </li>

  <% end %>

  <li>
      <a data-toggle="collapse" data-parent="#accordion" href="#collapseAdmin" aria-expanded="false" aria-controls="collapseAdmin">
        <i class="fa fa-star fa-fw" style="color:<%= color(1) %>" ></i> Admin Tools<span id="adminArrow" class="fa fa-chevron-circle-down pull-right" ></span></a>
      <ul id="collapseAdmin" class="nav nav-second-level collapse" >
        <li>
            <a href="<%= url_for :controller => 'employee', :action => 'index' %>">Employees</a>
        </li>
          <li>
              <a href="<%= url_for :controller => 'account_assignment', :action => 'index' %>">Account Assignment</a>
          </li>
          <li>
              <a href="<%= url_for :controller => 'category', :action => 'index' %>">Categories</a>
          </li>
          <li>
              <a href="<%= url_for :controller => 'admin', :action => 'category_hints' %>">Category Hints</a>
          </li>
          <li>
              <a href="<%= url_for :controller => 'admin', :action => 'supportlinks' %>">Support Links</a>
          </li>
      </ul>
      <!-- /.nav-second-level -->
  </li>


  <% else %>

  <li>
       <a data-toggle="collapse" data-parent="#accordion" href="#collapseDatasets" aria-expanded="true" aria-controls="collapseDatasets">
         <i class="fa fa-bars"></i> Datasets<span id="datasetsArrow" class="fa fa-chevron-circle-down pull-right"  style="display:none" ></span></a>
       <ul id="collapseDatasets" class="nav nav-second-level collapse in" aria-expanded="true">

         <li>
             <a <%= cssClassActive('part_detail') %> href="<%= url_for :controller => 'part_detail', :action => 'index' %>"><i class="fa fa-phone fa-fw" style="color:<%= color(5) %>" ></i> Part Detail </a>
         </li>




       </ul>


     </li>

  <li>
       <a data-toggle="collapse" data-parent="#accordion" href="#collapseTools" aria-expanded="true" aria-controls="collapseTools">
         <i class="fa fa-wrench fa-fw"></i> Actions<span id="toolsArrow" class="fa fa-chevron-circle-down pull-right"  style="display:none" ></span></a>
       <ul id="collapseTools" class="nav nav-second-level collapse in" aria-expanded="true">




           <li> <%= link_to "Checkout Parts", controller: "Checkout", action: "new"   %>    </li>
           <li> <%= link_to "Return Parts", controller: "Checkout", action: "return"   %>    </li>
