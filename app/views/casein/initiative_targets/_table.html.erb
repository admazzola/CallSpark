<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Bi targetted no", :bi_targetted_id %></th>
			<th><%= casein_sort_link "Bi targetted type", :bi_targetted_type %></th>
			<th><%= casein_sort_link "Initiative", :initiative_id %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% initiative_targets.each do |initiative_target| %>
		<tr>
			<td><%= casein_table_cell_link initiative_target.bi_targetted_id, casein_initiative_target_path(initiative_target) %></td>
			<td><%= casein_table_cell_link initiative_target.bi_targetted_type, casein_initiative_target_path(initiative_target) %></td>
			<td><%= casein_table_cell_link initiative_target.initiative_id, casein_initiative_target_path(initiative_target) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_initiative_target_path(initiative_target), :method => :delete, :data => { :confirm => "Are you sure you want to delete this initiative target?" }) %>
			</td>
		</tr>
	<% end %>
</table>